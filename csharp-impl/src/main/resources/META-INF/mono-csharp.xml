<consulo-plugin>
	<extensions defaultExtensionNs="com.intellij">
		<moduleExtensionProvider key="mono-csharp"
								 parentKey="mono-dotnet"
								 name="C#"
								 icon="/icons/mono.png"
								 immutableClass="consulo.mono.csharp.module.extension.MonoCSharpModuleExtension"
								 mutableClass="consulo.mono.csharp.module.extension.MonoCSharpMutableModuleExtension"/>
	</extensions>

	<extensions defaultExtensionNs="consulo.csharp">
		<compilerProvider id="mono-internal" implementation="consulo.mono.csharp.compiler.MonoInternalCompilerProvider" order="first"/>
	</extensions>
</consulo-plugin>