<idea-plugin>
	<extensions defaultExtensionNs="com.intellij">
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.obsolete.ObsoleteInspection"
						 displayName="Obsolete warnings" groupName="C#" enabledByDefault="true"/>
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.unnecessaryCast.UnnecessaryCastInspection"
						 displayName="Unnecessary Cast" groupName="C#" enabledByDefault="true"/>
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.unnecessaryType.UnnecessaryTypeInspection"
						 displayName="Unnecessary Type" groupName="C#" enabledByDefault="false"/>
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.unnecessaryModifier.UnnecessaryModifierInspection"
						 displayName="Unnecessary Modifier" groupName="C#" enabledByDefault="true"/>
		<localInspection
				language="C#"
				implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.unnecessaryNamedArgument.UnnecessaryNamedArgumentInspection"
				displayName="Unnecessary Named Argument" groupName="C#" enabledByDefault="true"/>
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.unusedSymbol.UnusedSymbolLocalInspection"
						 displayName="Unused Symbol" groupName="C#" enabledByDefault="true"/>
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.ide.codeInspection.unusedUsing.UnusedUsingInspection"
						 displayName="Unused using" groupName="C#" enabledByDefault="true"/>

		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.lang.doc.inspection.CSharpDocInspection"
						 displayName="C# Documentation problems" groupName="C# Documentation" enabledByDefault="true"/>
		<localInspection language="C#" implementationClass="org.mustbe.consulo.csharp.lang.doc.inspection.CSharpDocReferenceInspection"
						 displayName="C# Documentation reference problems" groupName="C# Documentation" enabledByDefault="true" level="WARNING"/>
	</extensions>
</idea-plugin>